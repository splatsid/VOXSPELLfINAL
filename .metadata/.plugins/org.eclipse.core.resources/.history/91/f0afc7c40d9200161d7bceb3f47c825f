package rewardsNStats;

import javafx.beans.property.SimpleStringProperty;

public class Word {
	
	private SimpleStringProperty word;
	private SimpleIntegerProperty correct;
	private int faulted;
	private int failed;
	
	
	Word(String word, int correct, int faulted, int failed){
		this.word.set(word);
		this.correct  = correct;
		this.faulted = faulted;
		this.failed = failed;
	}
	
	
	public String getWord(){
		return word;
	}
	
	public int getCorrect(){
		return correct;
	}
	
	public int getFailed(){
		return failed;
	}

}
